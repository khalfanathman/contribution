//  Sidebar 
ul#accordionSidebar.navbar-nav.bg-gradient-primary.sidebar.sidebar-dark.accordion
  //  Sidebar - Brand 
  a.sidebar-brand.d-flex.align-items-center.justify-content-center(href="index.html")
    .sidebar-brand-icon.rotate-n-15
      i.fas.fa-laugh-wink
    .sidebar-brand-text.mx-3
      | WelfareGroup
  //  Divider 
  hr.sidebar-divider.my-0
  //  Nav Item - Dashboard 
  li.nav-item
    a.nav-link(href="/")
      i.fas.fa-fw.fa-tachometer-alt
      span Dashboard
  //  Divider 
  hr.sidebar-divider
  //  Heading 
  .sidebar-heading.
    
    Participants
    
  //  Nav Item - Pages Collapse Menu 
  li.nav-item.active
    a.nav-link(href="#", data-toggle="collapse", data-target="#collapseTwo", aria-expanded="true", aria-controls="collapseTwo")
      i.fas.fa-fw.fa-cog
      span Members
    #collapseTwo.collapse.show(aria-labelledby="headingTwo", data-parent="#accordionSidebar")
      .bg-white.py-2.collapse-inner.rounded
        h6.collapse-header Group Members:
        a.collapse-item(href="buttons.html") Admins 
        a.collapse-item.active(href="/members") Users
  //  Nav Item - Utilities Collapse Menu 
  li.nav-item
    a.nav-link.collapsed(href="#", data-toggle="collapse", data-target="#collapseUtilities", aria-expanded="true", aria-controls="collapseUtilities")
      i.fas.fa-fw.fa-wrench
      span Wellnes & Activities
    #collapseUtilities.collapse(aria-labelledby="headingUtilities", data-parent="#accordionSidebar")
      .bg-white.py-2.collapse-inner.rounded
          h6.collapse-header Wellnes & Activities: 
            a.collapse-item(href="#") Contribute
            a.collapse-item(href=`/beneficiaries/${member.id}/`) Beneficiries 
            a.collapse-item(href=`/dependants/${member.id}`) Dependants   
  //  Divider 
  hr.sidebar-divider
  //  Heading 
  .sidebar-heading.
    
    Requests
    
  //  Nav Item - Pages Collapse Menu 
  li.nav-item
    a.nav-link.collapsed(href="#", data-toggle="collapse", data-target="#collapsePages", aria-expanded="true", aria-controls="collapsePages")
      i.fas.fa-fw.fa-folder
      span Requests
    #collapsePages.collapse(aria-labelledby="headingPages", data-parent="#accordionSidebar")
      .bg-white.py-2.collapse-inner.rounded
        h6.collapse-header Requests:
        a.collapse-item(href="#") Dependant Requests 
        a.collapse-item(href="#") Contribution Request
  //  Nav Item - Charts 
  li.nav-item
    a.nav-link(href="/savings")
      i.fas.fa-fw.fa-chart-area
      span Savings
  //  Nav Item - Tables 
  li.nav-item
    a.nav-link(href="tables.html")
      i.fas.fa-fw.fa-table
      span Tables
  //  Divider 
  hr.sidebar-divider.d-none.d-md-block
  //  Sidebar Toggler (Sidebar) 
  .text-center.d-none.d-md-inline
    button.rounded-circle.border-0#sidebarToggle

     //  Core plugin JavaScript
